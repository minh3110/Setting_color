<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Setting" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@android:color/background_dark</item>
        <item name="colorPrimaryVariant">@color/design_default_color_background</item>
        <item name="colorOnPrimary">@color/cardview_dark_background</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cardview_dark_background</item>
        <item name="colorSecondaryVariant">@color/cardview_dark_background</item>
        <item name="colorOnSecondary">@color/design_default_color_background</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>